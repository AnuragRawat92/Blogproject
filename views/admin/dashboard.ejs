<div class="admin-title">
    <h2>Posts</h2>
    <a href="/add-post" class="button">+ Add New</a>
</div>
<ul class="admin-posts">
    <% data.forEach(posts => { %>
     <li>
        <a href="/posts/<%= posts._id %>">
        <%= posts.title %>
    &nearr;
    </a>
    <div class="admin-post-controls">
        <a href="/edit-post/<%= posts._id %>" >Edit</a>
      
        <form action="/delete-post/<%= posts._id %>?_method=DELETE" method="POST">
        <input type="submit" value="Delete" class="btn"></form>
    </div>
     </li>
    <% }) %>
</ul>